{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true,
    "vue/setup-compiler-macros": true
  },
  "extends": ["eslint:recommended", "plugin:vue/essential"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "vue/no-multiple-template-root": "off"
  }
}
